{
  "name": "metarpheus-swagger",
  "version": "0.1.0",
  "description": "metarpheus interpreter producing Swagger specs",
  "main": "index.js",
  "scripts": {
    "start": "node --harmony-destructuring --harmony_default_parameters index.js input.json",
    "watch": "nodemon --harmony-destructuring --harmony_default_parameters index.js input.json",
    "test": "node --harmony-destructuring --harmony_default_parameters -- ./node_modules/ava/cli.js --verbose tests",
    "test-watch": "nodemon --harmony-destructuring --harmony_default_parameters -- ./node_modules/ava/cli.js --verbose tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/buildo/metarpheus-swagger.git"
  },
  "author": "buildo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/buildo/metarpheus-swagger/issues"
  },
  "homepage": "https://github.com/buildo/metarpheus-swagger#readme",
  "dependencies": {
    "argparse": "^1.0.6",
    "json2yaml": "^1.0.3",
    "object-assign-deep": "0.0.4"
  },
  "devDependencies": {
    "ava": "^0.12.0"
  }
}
